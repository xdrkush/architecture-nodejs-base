{
    "name": "architecture-nodejs-base",
    "version": "1.0.0",
    "description": "Tuto nodejs Basic",
    "main": "server.js",
    "scripts": {
        "test": "nodemon --exec 'mocha ./exercices/test.js'",
        "starter": "nodemon --exec 'mocha ./exercices/*starter.js'",
        "chai": "nodemon --exec 'mocha ./exercices/chai*.js'",
        "mocha": "nodemon --exec 'mocha ./exercices/mocha*.js'",
        "async": "nodemon --exec 'mocha ./exercices/async*.js'",
        "tu": "nodemon --exec 'mocha ./test/*.js --timeout 10000 -R min'",
        "start": "node server.js",
        "reinstall": "sudo rm -r node_modules* && npm i && npm start",
        "sass": "sass --watch ./public/css/sass/style.sass:./public/css/style.css",
        "dev": "nodemon server.js"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/xdrkush/architecture-nodejs-base.git"
    },
    "keywords": [],
