    "dependencies": {
        "body-parser": "^1.19.0",
        "connect-mongo": "^3.2.0",
        "date-format": "^3.0.0",
        "express": "^4.17.1",
        "express-handlebars": "^3.1.0",
        "express-session": "^1.17.0",
        "lodash": "^4.17.20",
        "method-override": "^3.0.0",
        "mongoose": "^5.8.9",
        "mysql": "^2.18.1",
        "node-cron": "^2.0.3"
    },
