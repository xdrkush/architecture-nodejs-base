<h1>Article</h1>

<div>
    <h2><u>Liste Article:</u></h2>
    <div class="flx">
        {{#each dbArticle }}
        <div class="itm">
            <h3>Titre: {{ title }}</h3>
        </div>
        {{/each }}
    </div>
</div>

<div>
    <form action="/article" method="post">
        <input type="text" name="title" placeholder="Title">
        <button type="submit">Confirmer</button>
    </form>
</div>